    <StaticWebAssetContentTypeMapping Include="application/octect-stream" Pattern="*.dat.br" Priority="8" />
    <StaticWebAssetContentTypeMapping Include="application/manifest+json" Pattern="*.webmanifest.br" Priority="8" />

