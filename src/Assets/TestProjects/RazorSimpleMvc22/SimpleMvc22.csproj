<Project Sdk="Microsoft.NET.Sdk.Web">

  <!--
    This project references a shipped version of MVC and should not reference local builds of
    the CodeGeneration targets, rzc, or any of the test shims.
  -->

  <PropertyGroup>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <CheckEolTargetFramework>false</CheckEolTargetFramework>
  </PropertyGroup>

  <!-- Test Placeholder -->

  <PropertyGroup>
    <!-- We don't want to run build server when not running as tests. -->
    <UseRazorBuildServer>false</UseRazorBuildServer>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.App" Version="2.2.6" PrivateAssets="All" />
    <PackageReference Include="Microsoft.AspNetCore.Razor.Design" Version="2.2.0" PrivateAssets="All" />
  </ItemGroup>

</Project>
