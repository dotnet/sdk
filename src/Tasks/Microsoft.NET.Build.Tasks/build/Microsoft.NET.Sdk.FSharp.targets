<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <Choose>
    <When Condition=" '$(MSBuildRuntimeType)' == 'core' ">
      <PropertyGroup>
         <FSharpTargetsShim>FSharp\FSharp\Microsoft.FSharp.targets</FSharpTargetsShim>
      </PropertyGroup>
    </When>
    <Otherwise>
      <PropertyGroup>
         <FSharpTargetsShim>$(MSBuildExtensionsPath32)\Microsoft\VisualStudio\v$(VisualStudioVersion)\Microsoft.FSharp.targets')</FSharpTargetsShim>
      </PropertyGroup>
    </Otherwise>
  </Choose>
  <Import Condition="Exists('$(FSharpTargetsShim)')" Project="$(FSharpTargetsShim)" />

</Project>
