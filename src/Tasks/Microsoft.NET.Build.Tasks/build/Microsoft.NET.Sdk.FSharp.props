<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

    <Choose>
        <When Condition=" '$(MSBuildRuntimeType)' == 'core' ">
             <Import Project="FSharp\Microsoft.NET.Sdk.FSharp.props" />
       </When>
       <Otherwise>
             <Import Project="$(MSBuildExtensionsPath32)\Microsoft\VisualStudio\v$(VisualStudioVersion)\Microsoft.NET.Sdk.FSharp.props" />
        </Otherwise>
    </Choose>

</Project>
