<!-- This file contains a list of the TPIs that are supported by this SDK. -->
<Project>
    <!--  -->
    <ItemGroup>
      <SupportedBaseSdkTargetPlatform Include="Android" />
      <SupportedBaseSdkTargetPlatform Include="iOS" />
      <SupportedBaseSdkTargetPlatform Include="macOS" />
      <SupportedBaseSdkTargetPlatform Include="tvOS" />
      <SupportedBaseSdkTargetPlatform Include="watchOS" />
      <SupportedBaseSdkTargetPlatform Include="Windows" />
    </ItemGroup>

    <!-- All supported target platforms -->
    <ItemGroup>
        <SupportedTargetPlatform Include="@(SupportedBaseSdkTargetPlatform)" />
    </ItemGroup>
</Project>
